
.home-top {
  height: 135px;
  padding-top: 14px;
  background: linear-gradient(to top, #1D4259, #122640);
  width: 100%;
  position: absolute;


}

.home_board {
  margin: 10px;
  display: flex;
  background: #fff;
  height: 20px;
  border-radius: 6px;
  padding: 12px;

}

.home_board_list{
  display: flex;
  padding-left: 5px;

}
.home_board_wz {
  transform: skewX(-30deg);
  width: 44px;
  height: 20px;
  background: linear-gradient(to top left, #f7ce9e, #d69c63);
  text-align: center;
  color: #fff;
  font-size: 13px;
  border-radius: 3px;
}
.home_board_wz_tr{
  display: inline-block;
  transform: skewX(30deg);

}
.home_board_wz2 {
  font-size: 13px;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-family: PingFang;
  font-weight: 400;
  margin-left: 10px;
  width: 80%;
  overflow: hidden;
  color: rgba(51, 51, 51, 1);
}

.bg_kl {
  background: #c16550;
  color: #fff
}

.bg_kl2 {
  background: #40749c;
  color: #fff
}



.bg_kl3 {
  background: #f2bf80;
  color: #fff
}


.home-list {
  margin: 10px;
  min-height: 400px;
  padding-top: 1px;
  background: #fff;
  margin-top: 0px;
}

.main_title {
  background-color: #fff;
  padding: 20rpx 0px;
  padding-bottom: 0px;
  margin: 0px 20rpx;
}
